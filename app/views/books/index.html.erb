<h1>Books</h1>
<% @books.each do |book| %>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <th>Show</th>
      <th>Edit</th>
      <th>Destroy</th>
    </tr>
  </tbody>
</table>
<% end %>
<h2>New book</h2>
